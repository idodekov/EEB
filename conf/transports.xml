<?xml version="1.0" encoding="UTF-8"?>
<Transports>
	<Transport type="http">
		<Protocol type="as2">
			<Setting name="direction">inbound</Setting>
			<Setting name="from">beta</Setting>
			<Setting name="to">alpha</Setting>
			<Setting name="productionFolder">F:\</Setting>
			<Setting name="sign">true</Setting>
			<Setting name="signCertAlias">sign1</Setting>
			<Setting name="encrypt">true</Setting>
			<Setting name="encryptCertAlias">encrypt1</Setting>
			<Setting name="encryptCertKeyPassword">redoubt</Setting>
		</Protocol>
		<Setting name="name">AS2 HTTP server</Setting>
		<Setting name="port">4080</Setting>
		<Setting name="tlsEnabled">false</Setting>
		<Setting name="contextPath">/as2</Setting>
	</Transport>
	<Transport type="file_system">
		<Protocol type="as2">
			<Setting name="direction">outbound</Setting>
			<Setting name="from">alpha</Setting>
			<Setting name="to">beta</Setting>
			<Setting name="url">http://localhost:4080/as2/</Setting>
			<Setting name="username">idodekov</Setting>
			<Setting name="password">SECRET123</Setting>
			<Setting name="sign">true</Setting>
			<Setting name="signCertAlias">sign1</Setting>
			<Setting name="signCertKeyPassword">redoubt</Setting>
			<Setting name="signDigestAlgorithm">md5</Setting>
			<Setting name="encrypt">false</Setting>
			<Setting name="encryptCertAlias">encrypt1</Setting>
			<Setting name="encryptAlgorithm">3des</Setting>
			<Setting name="compress">true</Setting>
			<Setting name="compressAlgorithm">zlib</Setting>
		</Protocol>
		<Setting name="folder">F:\WORK\___WORK</Setting>
		<Setting name="name">FileSystem</Setting>
		<Setting name="pollingInterval">2</Setting>
	</Transport>
</Transports>

